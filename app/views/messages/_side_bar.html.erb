<div class="side-bar-up">
  <div class="user-name">
    <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class="new-room">
    <%= link_to "チャットを作成する", new_room_path, class: "roomlink" %>
  </div>
  </div>
<div class="side-bar-dwn">
  <% current_user.rooms.each do |room| %>
    <div class="room">
      <div class="room-name">
          <%= link_to room.name, room_messages_path(room), class: "roomlink" %>
      </div>
    </div>
  <% end %>
</div>